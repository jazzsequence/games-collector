{
	"name": "fig-r/psr2r-sniffer",
	"license": "MIT",
	"description": "Code-Sniffer, Auto-Fixer and Tokenizer for PSR2-R",
	"authors": [
		{
			"name": "Mark Scherer",
			"homepage": "http://www.dereuromark.de",
			"role": "Contributor"
		}
	],
	"require": {
		"php": ">=5.4.16",
		"squizlabs/php_codesniffer": "~2.3"
	},
	"autoload": {
		"psr-4": {
			"PSR2R\\": "PSR2R"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"PSR2T\\Test\\": "tests"
		}
	},
	"bin": [
		"bin/tokenize",
		"bin/sniff"
	]
}
